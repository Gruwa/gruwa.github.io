{
    "root": {
        "src": "./src",
        "dev": "./build"
    },
    "html": {
        "src": "./",
        "dev": "./",
        "parts": "templates"
    },
    "—Åss": {
        "src": "style",
        "dev": "style",
        "parts": "parts",
        "extensions": "*.+(css|scss)"
    },
    "js": {
        "src": "js",
        "dev": "js",
        "parts": "modules",
        "extensions": [".json", ".js"]
    },
    "img": {
        "src": "img/**/*",
        "dev": "img",
        "svg": "img/svg",
        "extensions": "*.+(jpg|jpeg|gif|png|svg)"
    },
    "fonts": {
        "src": "font",
        "dev": "font",
        "extensions": "*.+(woff2|woff|eot|ttf|svg)"
    }
}
